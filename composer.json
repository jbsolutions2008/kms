{
    "name": "telegraph/kms",
    "type": "project",
    "description": "Kemp Management Solutions Repo",
    "homepage": "https://github.com/Telegraph-Creative/kms",
    "authors": [{
        "name": "John Cobbs",
        "email": "john@telegraphcreative.com"
    }],
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://wpackagist.org"
    }],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.4",
        "vlucas/phpdotenv": "^2.0.1",
        "johnpbloch/wordpress": "5.5.1",
        "oscarotero/env": "^1.1.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "wpackagist-plugin/backwpup": "3.7.1",
        "wpackagist-plugin/classic-editor": "1.6",
        "wpackagist-plugin/codepress-admin-columns": "4.2.2",
        "wpackagist-plugin/disable-emojis": "1.7.2",
        "wpackagist-plugin/disable-comments": "1.11.0",
        "wpackagist-plugin/post-types-order": "1.9.5.2",
        "wpackagist-plugin/wp-mail-smtp": "2.3.1",
        "wpackagist-plugin/wordpress-seo": "12.5"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": [
            "phpcs"
        ]
    }
}
